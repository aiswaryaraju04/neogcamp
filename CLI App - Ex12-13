var readlineSync = require("readline-sync");

var superman= {
  name: "Superman",
  power: "Flight",
  costumeColor: "Blue",
  strength : 1000,
  stealth : 0
}

var batman = {
  name : "Batman",
  power : "Martial Arts",
  costumeColor: "Black",
  strength : 1500,
  stealth : 100
}

console.log(superman.name)
console.log(batman.name)
console.log(superman.strength > batman.strength)
console.log(superman.stealth)
console.log(batman.stealth)
console.log(superman.stealth > batman.stealth)


var superheroes = [superman, batman];
for(var i=0;i<superheroes.length;i++)
{
  console.log(superheroes[i].name)
  console.log(superheroes[i].costumeColor)
}
